{
  "name": "instagram-auth",
  "version": "1.0.0",
  "description": "authentication",
  "main": "index.js",
  "repository": "https://github.com/WaifuForever/Instagram-Clone",
  "author": "Waifu_Forever",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:proto": "yarn dc:delete:folder proto/build && ./node_modules/.bin/proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/build proto/services/*.proto",
    "prestart": "yarn build",
    "start:dev": "NODE_ENV=dev tsnd --transpile-only --ignore-watch node_modules src/index.ts dotenv_config_path=./.env.dev",
    "dc:up": "sudo docker-compose --env-file .env.dev up",
    "dc:down": "sudo docker-compose --env-file .env.dev down",
    "dc:deepdown": "yarn dc:delete:volumes && yarn dc:delete:folder /usr/app2/v",
    "dc:stop": "sudo docker-compose --env-file .env.dev stop",
    "dc:delete:volumes": "sudo docker-compose --env-file .env.dev down -v",
    "dc:delete:folder": "sudo rm -rf",
    "dc:build": "sudo docker-compose --env-file .env.dev build",
    "dc:restart": "sudo docker restart insta-auth",
    "dc:stop:server": "sudo docker stop insta-auth"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.7",
    "@grpc/proto-loader": "^0.6.9",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "grpc-tools": "^1.11.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.12",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
